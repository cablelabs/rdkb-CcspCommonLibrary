[Unit]
Description=CcspLogent service

After=dbus.service CcspCrSsp.service

Requires=CcspCrSsp.service
Wants=dbus.service

[Service]
Type=forking
WorkingDirectory=/usr/ccsp/logagent
Environment="Subsys=eRT."
ExecStart=/usr/bin/log_agent -subsys $Subsys
Restart=always

StandardOutput=syslog

[Install]
WantedBy=multi-user.target
