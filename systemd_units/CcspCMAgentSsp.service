[Unit]
Description=CcspCMAgentSsp service
After=dbusOverride.service CcspCrSsp.service logagent.service PsmSsp.service CcspPandMSsp.service \
CcspWecbController.service emailNotificationMonitor.service CcspMtaAgentSsp.service
Requires=CcspCrSsp.service

[Service]
Type=forking
Environment="Subsys=eRT."
WorkingDirectory=/usr/ccsp/cm
ExecStart=/usr/bin/CcspCMAgentSsp -subsys $Subsys
Restart=always

StandardOutput=syslog

[Install]
WantedBy=multi-user.target
